{
    "manifest_version": 3,
    "name": "WITY (Who Is This Youtuber)",
    "version": "0.0.1",
    "description": "Descriptive pop up for a youtube channel",
    "content_scripts": [
    {
        "js": [
            "config/index.js",

            "scripts/api.ts",
            "scripts/wity.ts",
            "scripts/ui.js",
            "scripts/globals.js",
            "scripts/wordcloud2.js",
            "scripts/stopwords.js",
            "scripts/api.ts",
            "scripts/summary/index.js",
            
            "libs/cache/upstash.js",
            "libs/debounce/index.js",
            "libs/openai/completion.js"
        ],
        "css": [
            "css/profilecard.css"
        ],
        "matches": [
        "https://*.youtube.com/*"
        ]
    }
    ]
}